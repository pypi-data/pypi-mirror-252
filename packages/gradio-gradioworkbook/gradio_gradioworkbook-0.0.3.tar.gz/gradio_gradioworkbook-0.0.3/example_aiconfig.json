{
  "name": "my_aiconfig",
  "schema_version": "latest",
  "metadata": {
    "parameters": {},
    "models": {},
    "model_parsers": {
      "gpt2": "HuggingFaceTextGenerationTransformer",
      "mistralai/Mistral-7B-v0.1": "HuggingFaceTextGenerationTransformer"
    }
  },
  "description": "This is an AIConfig to test running a model.",
  "prompts": [
    {
      "name": "run_this_prompt",
      "input": "I was {{moving_action}} down the road when",
      "metadata": {
        "model": {
          "name": "gpt2",
          "settings": {
            "num_return_sequences": 1,
            "max_new_tokens": 200,
            "min_new_tokens": 150
          }
        },
        "parameters": {
          "moving_action": "walking",
          "unused_param": "pls delete me"
        }
      }
    },
    {
      "name": "test_prompt_1",
      "input": "Say this is a test",
      "metadata": {
        "model": {
          "name": "gpt-4",
          "settings": {
            "model": "gpt-4"
          }
        },
        "parameters": {},
        "remember_chat_context": true
      },
      "outputs": [
        {
          "output_type": "execute_result",
          "data": {
            "role": "assistant",
            "content": "This is a test."
          },
          "metadata": {}
        }
      ]
    }
  ]
}
